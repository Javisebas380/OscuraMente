{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "start": "npm run dev",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "reinstall": "npm run clean && npm install --legacy-peer-deps",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "npx expo prebuild --clean",
    "prebuild:dev": "EXPO_PUBLIC_APP_ENV=development npx expo prebuild --clean",
    "prebuild:prod": "EXPO_PUBLIC_APP_ENV=production npx expo prebuild --clean",
    "build:dev:ios": "EXPO_PUBLIC_APP_ENV=development eas build --profile development --platform ios",
    "build:dev:android": "EXPO_PUBLIC_APP_ENV=development eas build --profile development --platform android",
    "build:preview:ios": "EXPO_PUBLIC_APP_ENV=development eas build --profile preview --platform ios",
    "build:preview:android": "EXPO_PUBLIC_APP_ENV=development eas build --profile preview --platform android",
    "build:prod:ios": "EXPO_PUBLIC_APP_ENV=production eas build --profile production --platform ios",
    "build:prod:android": "EXPO_PUBLIC_APP_ENV=production eas build --profile production --platform android",
    "build:prod:all": "EXPO_PUBLIC_APP_ENV=production eas build --profile production --platform all",
    "validate:config": "node -e \"console.log('Environment:', process.env.EXPO_PUBLIC_APP_ENV || 'development'); console.log('Config validation would run here')\""
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo-google-fonts/merriweather": "^0.4.1",
    "@expo-google-fonts/montserrat": "^0.4.1",
    "@expo-google-fonts/playfair-display": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "54.0.20",
    "expo-application": "~7.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.13",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-tracking-transparency": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.496.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^15.7.0",
    "react-native-purchases": "^9.4.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
